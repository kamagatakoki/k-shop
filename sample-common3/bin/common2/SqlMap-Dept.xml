<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="GenreLgEntity">
  <select id="getGenreLg" resultType="entity.GenreLgEntity">
    SELECT GENRE_LG_CD AS genreLgCd  FROM GENRE_LG WHERE GENRE_LG_CD = '01'
  </select>
</mapper>