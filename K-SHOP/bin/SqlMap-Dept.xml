<sqlMap>
    <!-- 1件検索 -->
    <select id="getGenreLg" resultClass="entity.GenreLgEntity">
        SELECT GENRE_LG_CD AS genreLgCd  FROM GENRE_LG WHERE GENRE_LG_CD = '01'
    </select>
</sqlMap>